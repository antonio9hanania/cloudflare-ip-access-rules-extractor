{
  "manifest_version": 2,
  "name": "Cloudflare IP Access Rules Extraction",
  "version": "1.3",
  "description": "Extracts IP Access Rules from Cloudflare dashboard",
  "permissions": ["activeTab", "storage", "https://dash.cloudflare.com/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://dash.cloudflare.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
